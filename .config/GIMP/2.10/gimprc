# GIMP gimprc
# 
# This is your personal gimprc file.  Any variable defined in this file takes
# precedence over the value defined in the system-wide gimprc:
# /etc/gimp/2.0/gimprc
# Most values can be set within GIMP by changing some options in the
# Preferences dialog.

(global-brush no)
(global-dynamics no)
(global-pattern no)
(global-gradient no)
(default-image
    (width 1024)
    (height 1024)
    (unit pixels)
    (xresolution 300)
    (yresolution 300)
    (resolution-unit millimeters)
    (image-type rgb)
    (precision u8-gamma)
    (color-managed yes)
    (color-profile NULL)
    (fill-type background)
    (comment "Created with GIMP"))
(undo-preview-size small)
(thumbnail-size none)
(quick-mask-color (color-rgba 0.68055555555555558 0.35169649515287105 0.35169649515287105 0.5))
(import-add-alpha yes)
(import-raw-plug-in "${gimp_plug_in_dir}/plug-ins/file-raw-placeholder/file-raw-placeholder")
(show-paint-tool-cursor no)
(monitor-xresolution 142)
(monitor-yresolution 141)
(navigation-preview-size small)
(default-view
    (show-menubar no)
    (show-statusbar yes)
    (show-rulers yes)
    (show-scrollbars no)
    (show-selection yes)
    (show-layer-boundary yes)
    (show-canvas-boundary yes)
    (show-guides yes)
    (show-grid no)
    (show-sample-points yes)
    (snap-to-guides no)
    (snap-to-grid no)
    (snap-to-canvas no)
    (snap-to-path no)
    (padding-mode default)
    (padding-color (color-rgb 1 1 1))
    (padding-in-show-all no))
(devices-share-tool yes)
(max-new-image-size 512M)
(toolbox-wilber no)
(theme "System")
(fill-options
    (style solid)
    (antialias yes)
    (feather no)
    (feather-radius 10))
(stroke-options
    (style solid)
    (antialias yes)
    (feather no)
    (feather-radius 10)
    (method line)
    (width 6)
    (unit pixels)
    (cap-style butt)
    (join-style miter)
    (miter-limit 10)
    (dash-offset 0)
    (dash-info 0)
    (emulate-brush-dynamics no))


# end of gimprc
